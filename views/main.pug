doctype html
html
	head
		title Eduardo J. Velasco
		meta(charset='UTF-8')
		meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
		meta(name='viewport', content='width=device-width,initial-scale=1')
		meta(name='author', content='Eduardo Velasco')
		meta(name='description', content='Personal space of Eduardo Velasco')
		meta(name='keywords', content='Eduardo Velasco, Eduardo, Velasco, portfolio, work, personal')
